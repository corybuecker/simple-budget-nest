{
  "dependencies": {
    "@nestjs/common": "9.4.2",
    "@nestjs/core": "9.4.2",
    "@nestjs/platform-express": "9.4.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@types/node": "20.2.5",
    "@types/react": "18.2.9",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "esbuild": "0.17.19",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.12.0",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "scripts": {
    "start": "npx ts-node src/main.ts",
    "start:dev": "npx nodemon --ext ts,tsx",
    "build": "npm run tailwind:compile && npm run frontend:compile && npx tsc --build",
    "frontend:compile": "npx esbuild --format=esm --outdir=./public --bundle --sourcemap --splitting --external:react --external:react-dom --external:class-validator --external:class-transformer ./src/frontend",
    "tailwind:compile": "npx tailwindcss -i src/frontend/index.css -o ./public/app.css",
    "frontend:watch": "npm run frontend:compile -- --watch",
    "tailwind:watch": "npm run tailwind:compile -- --watch"
  },
  "private": true
}

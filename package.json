    "@nestjs/sequelize": "10.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "decimal.js": "10.4.3",
    "express-session": "1.17.3",
    "helmet": "7.0.0",
    "morgan": "1.10.0",
    "passport": "0.6.0",
    "passport-openidconnect": "0.1.1",
    "pg": "8.11.0",
    "pug": "3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.13.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sequelize": "6.32.1",
    "sequelize-typescript": "2.1.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@types/express": "4.17.17",
    "@types/express-session": "1.17.7",
    "@types/morgan": "1.9.4",
    "@types/node": "20.3.1",
    "@types/passport": "1.0.12",
    "@types/passport-openidconnect": "0.1.0",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@types/sequelize": "4.28.15",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "esbuild": "0.18.4",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "scripts": {
    "start": "npx ts-node src/main.ts",
    "start:dev": "npx nodemon --ext ts,tsx",
    "build": "npm run tailwind:compile && npm run frontend:compile && npx tsc --build",
    "frontend:compile": "npx esbuild --format=esm --outdir=./public --bundle --sourcemap --splitting --external:react --external:react-dom --external:class-validator --external:class-transformer --target=es2017 --minify ./src/frontend",
    "tailwind:compile": "npx tailwindcss -i src/frontend/index.css -o ./public/app.css",
    "frontend:watch": "npm run frontend:compile -- --watch",
    "tailwind:watch": "npm run tailwind:compile -- --watch"
  },
  "private": true
}
